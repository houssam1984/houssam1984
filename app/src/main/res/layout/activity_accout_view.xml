<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="accoutViewVM" type="com.houssameddinesapplication.app.modules.accoutview.data.viewmodel.AccoutViewVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnrectangle3809"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackgroup"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700">
            <FrameLayout android:id="@+id/frameStackrectangle3809"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_645pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageRectangle3809"
                    android:layout_gravity="start"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rectangle3809_645X414}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3809_645X414"/>
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarAppbar"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowarrowleft"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_15pxh"
                            android:layout_width="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft_deep_purple_A200"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_deep_purple_A200"/>
                        <ImageView android:id="@+id/imageGroup8916"
                            android:layout_height="@dimen/_5pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_group8916_deep_purple_A200"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_group8916_deep_purple_A200"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearColumngroup"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle_4">
                <ImageView android:id="@+id/imageGroup"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_130pxh"
                    android:layout_width="@dimen/_130pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnellipseeleven"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyledeep_purple_2">
                    <LinearLayout android:id="@+id/linearRowfollow"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/linearRowellipseeleven"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageEllipseEleven"
                                android:layout_height="@dimen/_50pxh"
                                android:layout_width="@dimen/_50pxh"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_ellipse11}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse11"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearColumnrosalia"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtRosalia"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{accoutViewVM.accoutViewModel.txtRosalia}"
                                    style="@style/txtIntersemibold20_1" tools:text="@string/lbl_rosalia"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtRoseTwentyThree"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{accoutViewVM.accoutViewModel.txtRoseTwentyThree}"
                                    style="@style/txtInterregular14" tools:text="@string/lbl_rose23"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnfollow"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:orientation="vertical" style="@style/groupStylecornerRadius_14">
                            <androidx.appcompat.widget.AppCompatButton
                                android:background="@drawable/rectangle_border_white_a700_radius_16_5"
                                android:ellipsize="end"
                                android:fontFamily="@font/inter"
                                android:gravity="center"
                                android:id="@+id/btnFollow"
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:paddingBottom="@dimen/_8pxv"
                                android:paddingEnd="@dimen/_24pxh"
                                android:paddingStart="@dimen/_24pxh"
                                android:paddingTop="@dimen/_8pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_follow"
                                android:textAllCaps="false"
                                android:textColor="@color/white_A700"
                                android:textFontWeight="400"
                                android:textSize="@dimen/_16pxh"
                                android:textStyle="bold" tools:text="@string/lbl_follow"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowpost"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnpost"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtPost"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{accoutViewVM.accoutViewModel.txtPost}"
                                style="@style/txtInterregular20_1" tools:text="@string/lbl_post"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtSeventyFive"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{accoutViewVM.accoutViewModel.txtSeventyFive}"
                                style="@style/txtIntersemibold20_1" tools:text="@string/lbl_75"/>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnfollowing"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtFollowing"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{accoutViewVM.accoutViewModel.txtFollowing}"
                                style="@style/txtInterregular20_1" tools:text="@string/lbl_following"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtZipcode"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{accoutViewVM.accoutViewModel.txtZipcode}"
                                style="@style/txtIntersemibold20_1" tools:text="@string/lbl_3400"/>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnfollowers"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtFollowers"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{accoutViewVM.accoutViewModel.txtFollowers}"
                                style="@style/txtInterregular20_1" tools:text="@string/lbl_followers"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtZipcodeOne"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{accoutViewVM.accoutViewModel.txtZipcodeOne}"
                                style="@style/txtIntersemibold20_1" tools:text="@string/lbl_6500"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearRowclock"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackclock"
                    android:layout_height="@dimen/_50pxv"
                    android:layout_width="@dimen/_50pxh" style="@style/groupStylewhite_1">
                    <ImageView android:id="@+id/imageClock"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_width="@dimen/_22pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_clock"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_clock"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStackvector"
                    android:layout_height="@dimen/_50pxv"
                    android:layout_width="@dimen/_50pxh" style="@style/groupStylewhite_1">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_17pxh"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStackcall"
                    android:layout_height="@dimen/_50pxv"
                    android:layout_width="@dimen/_50pxh" style="@style/groupStylewhite_1">
                    <ImageView android:id="@+id/imageCall"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_17pxh"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_call"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_call"/>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
