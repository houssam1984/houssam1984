<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="loginScreenVM" type="com.houssameddinesapplication.app.modules.loginscreen.data.viewmodel.LoginScreenVM"/>
    </data>
    <FrameLayout android:id="@+id/frameLoginScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyledeep_purple_A200">
        <LinearLayout android:id="@+id/linearRowrectangle3799"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius">
            <ImageView android:id="@+id/imageRectangle3799"
                android:layout_height="@dimen/_158pxh"
                android:layout_width="@dimen/_8pxh"
                android:scaleType="fitXY" cornerRadius="@{@dimen/_4pxh}"
                imageRes="@{@drawable/img_rectangle3799}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3799"/>
            <ImageView android:id="@+id/imageRectangle3799One"
                android:layout_height="@dimen/_158pxh"
                android:layout_width="@dimen/_122pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_12pxh}"
                imageRes="@{@drawable/img_rectangle3799_158X122}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3799_158X122"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnrectangle3803"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <ImageView android:id="@+id/imageRectangle3803"
                    android:layout_height="@dimen/_67pxh"
                    android:layout_width="@dimen/_122pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_12pxh}"
                    imageRes="@{@drawable/img_rectangle3803}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3803"/>
                <ImageView android:id="@+id/imageRectangle3800"
                    android:layout_height="@dimen/_158pxh"
                    android:layout_width="@dimen/_122pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_12pxh}"
                    imageRes="@{@drawable/img_rectangle3800}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3800"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageRectangle3802"
                android:layout_height="@dimen/_158pxh"
                android:layout_width="@dimen/_122pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_12pxh}"
                imageRes="@{@drawable/img_rectangle3802}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3802"/>
            <ImageView android:id="@+id/imageRectangle3801"
                android:layout_height="@dimen/_158pxh"
                android:layout_width="@dimen/_8pxh"
                android:scaleType="fitXY" cornerRadius="@{@dimen/_4pxh}"
                imageRes="@{@drawable/img_rectangle3801}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3801"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearColumnfindnewfriend"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyle">
            <TextView android:id="@+id/txtFindnewfriend"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_2pxh"
                android:text="@{loginScreenVM.loginScreenModel.txtFindnewfriend}"
                style="@style/txtInterbold32" tools:text="@string/msg_find_new_friend"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@{loginScreenVM.loginScreenModel.txtDescription}"
                style="@style/txtInterregular16" tools:text="@string/msg_with_billions_o"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowfacebook"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageFacebook"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_facebook"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_facebook"/>
                <LinearLayout android:id="@+id/linearColumnorloginwith"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtOrloginwith"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{loginScreenVM.loginScreenModel.txtOrloginwith}"
                        style="@style/txtInterregular16" tools:text="@string/lbl_or_login_with"/>
                    <LinearLayout android:gravity="end"
                        android:id="@+id/linearRowtwitter"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageTwitter"
                            android:layout_height="@dimen/_19pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_twitter"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_twitter"/>
                        <ImageView android:id="@+id/imageGoogle"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_width="@dimen/_37pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_google"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_google"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_white_a700_radius_25"
                android:ellipsize="end" android:fontFamily="@font/inter"
                android:gravity="center" android:id="@+id/btnLogIn"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_log_in"
                android:textAllCaps="false"
                android:textColor="@color/deep_purple_A200"
                android:textFontWeight="500"
                android:textSize="@dimen/_18pxh"
                android:textStyle="bold" tools:text="@string/lbl_log_in"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_border_white_a700_radius_25"
                android:ellipsize="end" android:fontFamily="@font/inter"
                android:gravity="center" android:id="@+id/btnSignUp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_sign_up"
                android:textAllCaps="false"
                android:textColor="@color/white_A700"
                android:textFontWeight="500"
                android:textSize="@dimen/_18pxh"
                android:textStyle="bold" tools:text="@string/lbl_sign_up"/>
        </LinearLayout>
    </FrameLayout>
</layout>
