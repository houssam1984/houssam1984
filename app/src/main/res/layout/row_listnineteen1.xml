<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listnineteen1RowModel" type="com.houssameddinesapplication.app.modules.stories.data.model.Listnineteen1RowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnnineteen"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" android:orientation="vertical">
        <FrameLayout android:id="@+id/frameStacknineteen"
            android:layout_height="@dimen/_144pxv" android:layout_width="@dimen/_147pxh">
            <ImageView android:id="@+id/imageNineteen"
                android:layout_gravity="start"
                android:layout_height="@dimen/_144pxh"
                android:layout_width="@dimen/_147pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_10pxh}"
                imageRes="@{@drawable/img_19_144X147}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_19_144X147"/>
            <LinearLayout android:id="@+id/linearColumnvideocameraOne"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylecornerRadius_9">
                <LinearLayout android:id="@+id/linearRowvideocamera"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyledeep_purple_A200cornerRadius">
                    <ImageView android:id="@+id/imageVideocameraOne"
                        android:layout_height="@dimen/_7pxh"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_videocamera"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_videocamera"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLIVEOne"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{listnineteen1RowModel.txtLIVEOne}"
                        style="@style/txtIntermedium10" tools:text="@string/lbl_live2"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtLIVEWeTheF"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{listnineteen1RowModel.txtLIVEWeTheF}"
            style="@style/txtIntersemibold14" tools:text="@string/msg_live_we_the_f"/>
        <TextView android:ellipsize="end" android:id="@+id/txtPrice"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{listnineteen1RowModel.txtPrice}"
            style="@style/txtInterregular12_1" tools:text="@string/msg_19_00_forg"/>
    </LinearLayout>
</layout>
