<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="accountVM" type="com.houssameddinesapplication.app.modules.account.data.viewmodel.AccountVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnrectangle3809"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameStackrectangle3809"
            android:layout_height="@dimen/_273pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageRectangle3809"
                android:layout_gravity="start"
                android:layout_height="@dimen/_273pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_rectangle3809_273X414}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3809_273X414"/>
            <LinearLayout android:id="@+id/linearColumnarrowleft"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle_3">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="center_horizontal"
                    android:id="@+id/toolbarAppbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowarrowleft"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_15pxh"
                            android:layout_width="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft_deep_purple_A200"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_deep_purple_A200"/>
                        <ImageView android:id="@+id/imageGroup8916"
                            android:layout_height="@dimen/_5pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_group8916_deep_purple_A200"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_group8916_deep_purple_A200"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <LinearLayout android:id="@+id/linearRow"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/linearRowellipseeleven"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageEllipseEleven"
                            android:layout_height="@dimen/_50pxh"
                            android:layout_width="@dimen/_50pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_ellipse11}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse11"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnrosalia"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtRosalia"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{accountVM.accountModel.txtRosalia}"
                                style="@style/txtIntersemibold20_1" tools:text="@string/lbl_rosalia"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtRoseTwentyThree"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{accountVM.accountModel.txtRoseTwentyThree}"
                                style="@style/txtInterregular14" tools:text="@string/lbl_rose23"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowcontrast"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageButton
                            android:background="@drawable/rectangle_border_white_a700_radius_16_5"
                            android:id="@+id/btnContrast"
                            android:layout_height="@dimen/_33pxh"
                            android:layout_width="@dimen/_33pxh"
                            android:paddingHorizontal="@dimen/_5pxh"
                            android:paddingVertical="@dimen/_5pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_contrast" tools:ignore="ContentDescription"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:background="@drawable/rectangle_border_white_a700_radius_16_5"
                            android:ellipsize="end"
                            android:fontFamily="@font/inter"
                            android:gravity="center"
                            android:id="@+id/btnFollow"
                            android:layout_height="@dimen/_33pxv"
                            android:layout_width="@dimen/_76pxh"
                            android:paddingBottom="@dimen/_9pxv"
                            android:paddingEnd="@dimen/_16pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_9pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_follow"
                            android:textAllCaps="false"
                            android:textColor="@color/white_A700"
                            android:textFontWeight="400"
                            android:textSize="@dimen/_14pxh"
                            android:textStyle="bold" tools:text="@string/lbl_follow"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearAccount"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:id="@+id/linearRowpost"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnpost"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtPost"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{accountVM.accountModel.txtPost}"
                        style="@style/txtInterregular20" tools:text="@string/lbl_post"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtSeventyFive"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{accountVM.accountModel.txtSeventyFive}"
                        style="@style/txtIntersemibold20_3" tools:text="@string/lbl_75"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnfollowing"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtFollowing"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{accountVM.accountModel.txtFollowing}"
                        style="@style/txtInterregular20" tools:text="@string/lbl_following"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtZipcode"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{accountVM.accountModel.txtZipcode}"
                        style="@style/txtIntersemibold20_3" tools:text="@string/lbl_3400"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnfollowers"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtFollowers"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{accountVM.accountModel.txtFollowers}"
                        style="@style/txtInterregular20" tools:text="@string/lbl_followers"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtZipcodeOne"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{accountVM.accountModel.txtZipcodeOne}"
                        style="@style/txtIntersemibold20_3" tools:text="@string/lbl_6500"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowmenu"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_100">
                <ImageView android:id="@+id/imageMenu"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_menu"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                <ImageView android:id="@+id/imageMinimize"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_minimize"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_minimize"/>
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_33pxh"
                    android:layout_width="@dimen/_33pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_bluegray_400"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_bluegray_400"/>
                <ImageView android:id="@+id/imageMusic"
                    android:layout_height="@dimen/_23pxh"
                    android:layout_width="@dimen/_31pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_music"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_music"/>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerAccount"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_16pxh}" tools:itemCount="2" tools:listitem="@layout/row_account"/>
        </LinearLayout>
    </LinearLayout>
</layout>
